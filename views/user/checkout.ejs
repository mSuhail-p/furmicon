        <script>
            // Form validation and submission
            document.getElementById('newAddress').addEventListener('submit', function (event) {
                event.preventDefault();
                const form = event.target;
                let isValid = true;
                console.log('start reached');

                // Clear previous error messages
                form.querySelectorAll('.error-message').forEach(errorDiv => {
                    errorDiv.textContent = '';
                });
                form.querySelectorAll('.is-invalid').forEach(input => {
                    input.classList.remove('is-invalid');
                });

                // Validate each input field
                const inputs = [
                    { id: 'firstName', message: 'First name is required' },
                    { id: 'lastName', message: 'Last name is required' },
                    { id: 'country', message: 'Country is required' },
                    { id: 'streetAddress1', message: 'Street address is required' },
                    { id: 'city', message: 'Town/City is required' },
                    { id: 'state', message: 'State/County is required' },
                    { id: 'postcode', message: 'Postcode/ZIP is required' },
                    { id: 'phone', message: 'Phone number is required' },
                    { id: 'email', message: 'Email address is required' }
                ];

                inputs.forEach(input => {
                    const field = document.getElementById(input.id);
                    if (!field.value.trim()) {
                        isValid = false;
                        const errorDiv = field.nextElementSibling;
                        errorDiv.textContent = input.message;
                        field.classList.add('is-invalid');
                    }
                });

                valid = isValid;

                if (isValid) {
                    // Form submission logic here
                    // alert('Form submitted successfully!');
                }
            });

            // Function for getting selected address's ID from checkbox
            let id;
            function selection(index) {
                id = document.getElementById(`address-${index}`).value;
            }

            // Button for placing order
            document.getElementById('placeOrderButton').addEventListener('click', function (event) {
                // event.preventDefault();
                let formData;
                if (id == undefined && valid == true) {
                    formData = document.getElementById('newAddress');
                }

                let form = new FormData(formData);
                let formObject = {};
                for (const [key, value] of form.entries()) {
                    formObject[key] = value;
                }
                const totalAmount = document.getElementById('totalAmount').innerHTML
                const walletAmount = document.getElementById('walletAmount').value
                console.log(walletAmount, 'it is wallset amount')


                const selectedPaymentMethod = document.querySelector('input[name="paymentMethod"]:checked').value;
                console.log(selectedPaymentMethod);

                // Taking table data on the checkout page
                let tableData = [];
                const table = document.getElementById('dataTable');
                const rows = table.getElementsByTagName('tbody')[0].getElementsByTagName('tr');

                for (let i = 0; i < rows.length; i++) {
                    const cells = rows[i].getElementsByTagName('td');
                    let rowData = {
                        item: cells[0].innerText,
                        quantity: cells[1].innerText,
                    };
                    tableData.push(rowData);
                }
                console.log(tableData, 'it is table data');

                // If id is undefined, new address will be taken
                if (selectedPaymentMethod == 'Cash on delivery') {
                    handleCashOnDelivery(id, formObject, tableData);
                } else if (selectedPaymentMethod == 'Online Payment') {
                    handleOnlinePayment(id, formObject, tableData);
                } else if (selectedPaymentMethod == 'wallet') {
                    if (parseInt(totalAmount) <= walletAmount) {

                        handleWalletPayment(id, formObject, tableData)
                    } else {
                        Swal.fire({
                            icon: 'error',
                            title: 'Insufficient Balance',
                            text: 'You do not have enough balance to complete this transaction.',
                            confirmButtonText: 'OK'
                        });
                    }

                }
            });

            function handleCashOnDelivery(id, formObject, tableData) {
                if (id == undefined) {
                    let dataToSend = {
                        paymentMethod: 'Cash on delivery',
                        tableData: tableData,
                        formData: formObject,
                    };
                    console.log(dataToSend);
                    axios.post('/placeOrder', dataToSend)
                        .then(response => {
                            const data = response.data.codSuccess;
                            if (data) {
                                window.location.href = `/orderPlaced/${response.data.orderId}`;
                            }
                        })
                        .catch(error => {
                            console.log(error);
                        });
                } else {
                    let dataToSend = {
                        paymentMethod: 'Cash on delivery',
                        tableData: tableData,
                        addressId: id
                    };
                    axios.post('/placeOrder', dataToSend)
                        .then(response => {
                            const data = response.data.codSuccess;
                            if (data) {
                                window.location.href = `/orderPlaced/${response.data.orderId}`;
                            } else {
                                Swal.fire({
                                    position: "top-end",
                                    icon: "error",
                                    title: "Currently this method is not working",
                                    showConfirmButton: false,
                                    timer: 1500
                                });
                            }
                        })
                        .catch(error => {
                            console.log(error);
                        });
                }
            }

            function handleOnlinePayment(id, formObject, tableData) {
                if (id == undefined) {
                    let dataToSend = {
                        paymentMethod: 'Online Payment',
                        tableData: tableData,
                        formData: formObject,
                    };
                    console.log('it is formdata')
                    axios.post('/placeOrder', dataToSend)
                        .then(response => {
                            console.log(response.data.orderData, 'it is formdata')
                            razorpayPayment(response.data.order, response.data.orderData);
                        })
                        .catch(error => {
                            console.log(error);
                        });
                } else {
                    let dataToSend = {
                        paymentMethod: 'Online Payment',
                        tableData: tableData,
                        addressId: id
                    };
                    axios.post('/placeOrder', dataToSend)
                        .then(response => {
                            console.log('here is reached')
                            razorpayPayment(response.data.order, response.data.orderData);
                        })
                        .catch(error => {
                            console.log(error);
                        });
                }
            }


            function handleWalletPayment(id, formObject, tableData) {
                if (id == undefined) {
                    let dataToSend = {
                        paymentMethod: 'Wallet',
                        tableData: tableData,
                        formData: formObject,
                    };
                    console.log('it is formdata')
                    axios.post('/placeOrder', dataToSend)
                        .then(response => {
                            if (response.data.Success) {
                                window.location.href = `/orderPlaced/${response.data.orderId}`
                            }

                        })
                        .catch(error => {
                            console.log(error);
                        });
                } else {
                    let dataToSend = {
                        paymentMethod: 'Wallet',
                        tableData: tableData,
                        addressId: id
                    };
                    axios.post('/placeOrder', dataToSend)
                        .then(response => {
                            if (response.data.Success) {
                                window.location.href = `/orderPlaced/${response.data.orderId}`
                            }

                        })
                        .catch(error => {
                            console.log(error);
                        });
                }
            }


            function razorpayPayment(order, orderData) {
                console.log("here is reached, razorpayPayment");
                var options = {
                    "key": "rzp_test_m31KrIXyLXS9oV", // Enter the Key ID generated from the Dashboard
                    "amount": order.amount, // Amount is in currency subunits. Multiply by 100 for paise if order.amount is in INR
                    "currency": "INR",
                    "name": "Furmicon", // Your business name
                    "description": "Quality is never compromised",
                    "order_id": order.id, // This is a sample Order ID. Pass the id obtained in the response of Step 1
                    "handler": function (response) {
                        console.log("Payment successful. Response:", response);

                        const data = {
                            response,
                            order,
                            orderData

                        };

                        axios.post('/verifyPayment', data)
                            .then(response => {
                                if (response.data.Success) {
                                    window.location.href = `orderPlaced/${response.data.orderId}`
                                }
                            })
                            .catch(error => {
                                console.log("Verification error:", error);
                            });
                    },
