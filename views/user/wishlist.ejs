    <!-- ADD TO CART -->

    <script>


        function addCart(productId) {
            event.preventDefault();
            console.log("it is working", productId)

            let quantity = 1

            fetch(`/addToCart/${productId}/${quantity}`, {
                method: "GET",
                headers: {
                    'Content-Type': 'application/json'
                }
            })
                .then(response => {
                    if (!response.ok) {
                        throw new Error('Network response was not ok');
                    }
                    return response.json();
                })
                .then(data => {
                    if (data.status === true) {
                        Swal.fire({

                            icon: "success",
                            title: "Your work has been saved",
                            showConfirmButton: false,
                            timer: 1500
                        })
                    } else {
                        Swal.fire({
                            title: 'Item Already in Cart',
                            text: 'You already added this item to the cart.',
                            icon: 'info',
                            confirmButtonText: 'OK'
                        })
                    }
                })
                .catch(error => {
                    console.log("it is cotch")
                    console.error('Error:', error);
                });

        }


        // REMOVE FROM WISHLIST

        function remove(productID ,index) {

            

            axios.delete(`/removeWishlist/${productID}`)
                .then(Response => {
                   if(Response.data.removed){
                    document.getElementById(`wdocument-${index}`).remove()
                   }
                })

                .catch(error => {
                    console.log(error)
                })


        }



    </script>



